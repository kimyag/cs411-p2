{
  "manifest_version": 3,
  "name": "Bookmark Extension",
  "version": "1.0",
  "description": "A simple extension to add and view bookmarks.",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage", "bookmarks"],
  "background": {
    "service_worker": "background.js"
  }
}

